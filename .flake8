[flake8]
max-line-length = 100
extend-ignore = E203, E266, E501, W503, F401, F403
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    .eggs,
    *.egg,
    build,
    dist,
    .mypy_cache,
    .pytest_cache
max-complexity = 10
per-file-ignores =
    __init__.py:F401
    app/models/*.py:F401
